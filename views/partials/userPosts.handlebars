{{#each userPosts as |userPost|}}
<div class="row">
  <div class="col-md-3"></div>
  <div class="text-center border mx-3 mt-3 col-md-6">
    <div class="mt-2">
      <a href="/posts/admin/{{userPost.id}}"><h2>{{userPost.title}}</h2></a>
      <p class="mb-2">Created: {{format_date userPost.date_created}}</p>
    </div>
  </div>
  <div class="col-md-3"></div>
</div>
{{/each}}